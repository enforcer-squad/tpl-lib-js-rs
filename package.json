{
  "name": "tpl-lib-js-rs",
  "version": "1.0.0",
  "description": "javascript library template with rspack",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "keywords": [
    "library",
    "javascript",
    "template",
    "rspack"
  ],
  "author": "enforcer.a06@gmail.com",
  "license": "MIT",
  "packageManager": "pnpm@9.13.0",
  "scripts": {
    "prepare": "husky install",
    "start": "cross-env NODE_ENV=development rspack serve --config ./build/dev.mjs",
