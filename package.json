    "@rspack/cli": "1.1.1",
    "@rspack/core": "1.1.1",
    "@rspack/plugin-react-refresh": "1.0.0",
    "antd": "4.24.16",
    "cross-env": "7.0.3",
    "eslint": "9.15.0",
    "eslint-plugin-react": "7.37.2",
    "glob": "11.0.0",
    "globals": "15.12.0",
    "husky": "9.1.7",
    "less-loader": "12.2.0",
    "prettier": "3.3.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-refresh": "0.14.2",
    "react-router-dom": "6.28.0",
    "webpack-merge": "6.0.1",
    "webpack-node-externals": "3.0.0"
  },
  "dependencies": {
    "core-js": "3.39.0"
  }
}
